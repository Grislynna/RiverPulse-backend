{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "river-pulse-data-fetcher",
	"main": "src/index.js",
	"compatibility_date": "2025-11-11",
	"observability": {
		"enabled": true
	},
	"d1_databases": [
    {
      "binding": "backend-database-binding",
      "database_name": "river-data"
    }
  ],
	"triggers": {
		"crons": [
			"*/15 * * * *"
		]
	}
}