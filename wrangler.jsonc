{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "river-pulse-data-fetcher",
	"main": "src/index.js",
	"compatibility_date": "2025-11-11",
	"observability": {
		"enabled": true
	},
	"bindings": [
    		{
			"type": "d1",
			"name": "backend-database-binding",
			"database_name": "river-data"
		}
	],
	"triggers": {
		"crons": [
			"*/15 * * * *"
		]
	}
}
